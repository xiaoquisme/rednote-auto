# Twitter scraping (no API key needed)
TWITTER_TARGET_USERNAMES=["username1","username2"]
TWITTER_HEADLESS=true

# OpenAI API
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4o
OPENAI_BASE_URL=  # 可选，自定义 API 网关地址

# WeChat Official Account
WECHAT_APP_ID=your_wechat_app_id
WECHAT_APP_SECRET=your_wechat_app_secret

# XHS (小红书)
XHS_HEADLESS=true
XHS_BROWSER_STATE_DIR=data/browser_state

# Database
DATABASE_URL=sqlite+aiosqlite:///data/posts.db

# Inngest
INNGEST_APP_ID=rednote-auto
INNGEST_IS_PRODUCTION=false
