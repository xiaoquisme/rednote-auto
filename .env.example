# Anthropic API (required for Claude Code agents)
ANTHROPIC_API_KEY=your_anthropic_api_key

# Twitter settings (no API key needed, agent browses x.com)
TWITTER_TARGET_USERNAMES=["username1","username2"]

# Agent settings
AGENT_MODEL=sonnet
AGENT_MAX_TURNS=30
AGENT_HEADLESS=true

# Database
DATABASE_URL=sqlite+aiosqlite:///data/posts.db

# Inngest
INNGEST_APP_ID=rednote-auto
INNGEST_IS_PRODUCTION=false
